<div id="header" class="round">
<% logo = image_tag("logo.png", :alt => "zVault Coud App2", :class => "round") %>
<%= link_to logo, root_path %>
<ul class="navigation round">
<li><%= link_to "Home", root_path %></li>
<li><%= link_to "Company", company_path %></li>
<li><%= link_to "Product", product_path %></li>
<li><%= link_to "Services", services_path %></li>
<% if signed_in? %>
<li><%= link_to "Sign out", signout_path, :method => :delete %></li>
<% else %>
<li><%= link_to "Sign in", signin_path %></li>
<% end %>
</ul>
</div>